<div class="container-pag">
	<div class="row-title">
		<section class="event-section">
		<div id= "cont-event">
			  <div class="evento-header snow">
			  	<h1>Events</h1>
  				<% if current_user.admin? %>
     	 				<%= link_to "Add New Event", new_event_path, class: 'link btn btn-primary btns ml-4 showuser" style="height: 45px;position:absolute; left:700px;' %>
  				<%end%>
  			</div>
			<%if @events.count > 0%>
				<section class="all_events">
					<% @events.each do |event| %>
						<%= render event %>
					<%end%>
				</section>
			<%end%>
		</div>
</section>
		<div class="categoria">
			<%= render :partial => '/categories/categories'%>
		</div>
	</div>
</div>
<%= form_for :rating do |f| %>
    <% @user ||= current_user %>
    <%= f.hidden_field :event_id, :value => event_id %>
    <%= f.hidden_field :user_id, :value => user_id %>
    <div class="star-rating">
    <div class="rating">
        <% %w(1 2 3 4 5).each do |star| %>
            <%= f.radio_button :star, star , id:"r#{star}#{event_id}"%>
            <label for='<%="r#{star}#{event_id}"%>'></label>
        <%end%>
    </div>
   </div> 
<% end %>